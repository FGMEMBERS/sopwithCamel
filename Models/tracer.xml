<?xml version="1.0"?>
<!-- Red Tracer -->
<PropertyList>
  <path>tracer2.ac</path>

	<animation>
		<type>scale</type>
		<object-name>tracer</object-name>
		<x-factor>1</x-factor>
		<x-offset>0.1</x-offset>
		<y-factor>1</y-factor>
		<y-offset>0.1</y-offset>
		<z-factor>1</z-factor>
		<z-offset>0.1</z-offset>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
	</animation>

	<animation>
    <type>dist-scale</type>
    <object-name>tracer</object-name>
    <factor>6.0</factor>
    <offset>0</offset>
		<min>1.0</min>
		<max>6.0</max>
		<center>
      <x-m>0.0</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
    </center>
  </animation>
	

	<animation>
		<type>material</type>
		<object-name>tracer</object-name>
		<emission>
			<factor>1</factor>
			<offset>1</offset>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>
	
</PropertyList>
